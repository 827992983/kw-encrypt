<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="kwencryptFrame">
		<title>kw-encryptor</title>
		<size>932,491</size>
		<id_arg>0</id_arg>
		<style>wxCAPTION|wxRESIZE_BORDER|wxCLOSE_BOX|wxMAXIMIZE_BOX|wxMINIMIZE_BOX|wxCLIP_CHILDREN</style>
		<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
			<object class="wxBoxSizer" variable="BoxSizer1" member="no">
				<object class="sizeritem">
					<object class="wxPanel" name="ID_PANEL2" variable="Panel2" member="yes">
						<object class="wxBoxSizer" variable="BoxSizer2" member="no">
							<object class="sizeritem">
								<object class="wxListBox" name="ID_LISTBOX1" variable="ListBox1" member="yes">
									<default>-1</default>
									<tooltip>Drag and drop files here to add files</tooltip>
									<style>wxLB_EXTENDED|wxLB_NEEDED_SB|wxLB_SORT</style>
									<handler function="OnListBox1Select" entry="EVT_LISTBOX" />
								</object>
								<flag>wxEXPAND</flag>
								<option>1</option>
							</object>
						</object>
					</object>
					<flag>wxALL|wxEXPAND</flag>
					<border>5</border>
					<option>5</option>
				</object>
				<object class="sizeritem">
					<object class="wxBoxSizer" variable="BoxSizer3" member="no">
						<orient>wxVERTICAL</orient>
						<object class="sizeritem">
							<object class="wxButton" name="ID_BUTTON2" variable="btnAdd" member="yes">
								<label>Add</label>
								<handler function="OnbtnAddClick" entry="EVT_BUTTON" />
							</object>
							<flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND</flag>
							<border>1</border>
						</object>
						<object class="sizeritem">
							<object class="wxButton" name="ID_BUTTON3" variable="btnRemove" member="yes">
								<label>Remove</label>
								<handler function="OnbtnRemoveClick" entry="EVT_BUTTON" />
							</object>
							<flag>wxALL|wxEXPAND</flag>
							<border>1</border>
						</object>
						<object class="sizeritem">
							<object class="wxButton" name="ID_BUTTON1" variable="btnRemoveAll" member="yes">
								<label>Remove all</label>
								<handler function="OnbtnRemoveAllClick" entry="EVT_BUTTON" />
							</object>
							<flag>wxALL|wxEXPAND</flag>
							<border>1</border>
						</object>
						<object class="sizeritem">
							<object class="wxStaticLine" name="ID_STATICLINE1" variable="StaticLine1" member="yes">
								<size>10,-1</size>
							</object>
							<flag>wxALL|wxEXPAND</flag>
							<border>3</border>
						</object>
						<object class="sizeritem">
							<object class="wxButton" name="ID_BUTTON4" variable="btnEncrypt" member="yes">
								<label>Encrypt</label>
								<enabled>0</enabled>
								<handler function="OnbtnEncryptClick" entry="EVT_BUTTON" />
							</object>
							<flag>wxALL|wxEXPAND</flag>
							<border>1</border>
						</object>
					</object>
					<flag>wxTOP|wxBOTTOM|wxRIGHT|wxEXPAND</flag>
					<border>5</border>
					<option>1</option>
				</object>
			</object>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
		<object class="wxFileDialog" variable="FileDialog1" member="yes">
			<message>Select file</message>
			<style>wxFD_OPEN|wxFD_FILE_MUST_EXIST|wxFD_MULTIPLE|wxFD_CHANGE_DIR</style>
		</object>
	</object>
</wxsmith>
